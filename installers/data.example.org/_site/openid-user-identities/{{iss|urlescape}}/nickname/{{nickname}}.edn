{:deps
 ["https://auth.example.org/operations/register-openid-user-identity"
  "https://auth.example.org/permissions/system/register-openid-user-identity"]
 :install
 {:juxt.site/subject-uri "https://auth.example.org/_site/subjects/system"
  :juxt.site/operation-uri "https://auth.example.org/operations/register-openid-user-identity"
  :juxt.site/input
  {:xt/id "{{$id}}"
   :fullname "{{fullname}}"
   :juxt.site/user "{{user}}"
   :juxt.site.jwt.claims/iss "{{iss}}"
   :juxt.site.jwt.claims/nickname "{{nickname}}"}}}
