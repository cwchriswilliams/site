{:deps
 ["https://auth.example.org/operations/oauth/register-client"
  "https://auth.example.org/permissions/system/register-client"]
 :install
 {:juxt.site/subject-id "https://auth.example.org/_site/subjects/system"
  :juxt.site/operation-id "https://auth.example.org/operations/oauth/register-client"
  :juxt.site/input
  {:juxt.site/client-id "remote-swagger-ui"
   ;; This can be confidential because we can provide a client_secret
   ;; as an input in its Authorize dialog, rather than embedding the
   ;; client_secret in the swagger-ui application code or
   ;; configuration.
   :juxt.site/client-type "confidential"
   :juxt.site/origin "https://petstore.swagger.io"
   :juxt.site/authorization-server "https://auth.example.org"
   :juxt.site/resource-server "https://data.example.org"
   :juxt.site/redirect-uris ["https://petstore.swagger.io/oauth2-redirect.html"]}}}
